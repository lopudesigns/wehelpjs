{
  "name": "wehelpjs",
  "version": "0.6.60",
  "description": "wehelpjs is the JavaScript API Library for the WeYouMe blockchain",
  "main": "lib/index.js",
  "scripts": {
    "test": "eslint --quiet src test; mocha -t 40000 --require babel-polyfill --require babel-register",
    "test-auth": "npm test --grep 'webuilder'",
    "build": "npm run build-browser && npm run build-node",
    "build-browser": "rm -rf dist && webpack -p --config webpack.config.js && du -h ./dist/*",
    "build-node": "mkdir -p ./lib && cp -r ./src/* ./lib/ && babel ./src --experimental --out-dir ./lib && du -h ./lib/*",
    "prepare": "npm run build",
    "push": "git add . ; git commit -m 'ready' ; git push origin dev",
    "pull": "git pull origin dev"
  },
  "browser": {
    "ws": false,
    "crypto": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeYouMe/wehelpjs.git"
  },
  "keywords": [
    "WeYouMenetwork",
    "WeYouMe",
    "WeYouMe",
    "blockchain",
    "wehelpjs"
  ],
  "author": "Fabien (https://github.com/bonustrack)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WeYouMe/wehelpjs/issues"
  },
  "homepage": "https://github.com/WeYouMe/wehelpjs#readme",
  "dependencies": {
    "bigi": "^1.4.2",
    "bluebird": "^3.4.6",
    "browserify-aes": "^1.0.6",
    "bs58": "^4.0.0",
    "buffer": "^5.0.6",
    "bytebuffer": "^5.0.1",
    "change-case": "^3.0.2",
    "create-hash": "^1.1.2",
    "create-hmac": "^1.1.4",
    "cross-env": "^5.0.0",
    "cross-fetch": "^1.1.1",
    "debug": "^2.6.8",
    "detect-node": "^2.0.4",
    "diacritics": "^1.3.0",
    "ecurve": "^1.0.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "secure-random": "^1.1.1",
    "validator": "^10.8.0",
    "wecryptojs": "^1.2.38",
    "werpcjs": "^1.2.136",
    "ws": "^3.3.3",
    "webpack-sources": "1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "dotenv": "^6.1.0",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "mocha-make-stub": "^2.3.2",
    "should": "^11.1.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "contributors": [
    "Lopu (https://github.com/lopudesigns)"
  ]
}
